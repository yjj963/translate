<view class='container change'>
  <view class='title'>翻译成</view>
  <view class='item' wx:for='{{langList}}' wx:key='index' wx:for-item='language' bindtap='onTapItem' data-chs='{{language.chs}}' data-lang='{{language.lang}}' data-index='{{language.index}}'>
    <view class='text'>{{language.chs}}</view>
    <view class='iconfont icon' wx:if='{{index===curLang.index}}'>√</view>
  </view>
</view>